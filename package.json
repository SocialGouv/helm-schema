{
  "name": "@socialgouv/helm-schema",
  "version": "1.2.4",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "bin",
    "package.json",
    "yarn.lock",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": "./bin/index.js",
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "dev": "tsc --watch --outDir build",
    "build": "ncc build ./src/index.ts",
    "test": "jest",
    "snapshots": "yarn build && yarn test -u && ./bin/index.js -f .github/e2e/values1.yaml > .github/e2e/values1.schema.json && ./bin/index.js -f .github/e2e/values2.yaml > .github/e2e/values2.schema.json",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "comment-parser": "^1.4.0",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/json-schema": "^7.0.12",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.48.0",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "tsc": "^2.0.4",
    "typescript": "^5.2.2"
  }
}
